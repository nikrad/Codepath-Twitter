OTHER_LDFLAGS = $(inherited) -framework "AFNetworking" -framework "BDBOAuth1Manager" -framework "CCInfiniteScrolling" -framework "DProperty" -framework "DateTools" -framework "JRSwizzle" -framework "SwiftyJSON" -framework "UIView_TKGeometry"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Twitter
PODS_ROOT = ${SRCROOT}/Pods